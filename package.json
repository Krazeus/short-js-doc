{
  "name": "short-js-doc",
  "version": "0.1.8",
  "publisher": "myax",
  "displayName": "Short JS Doc",
  "description": "Short Javascript Snippets and Documentation based on JSDuck.  \n A collection of Javascript snippets for faster developmment in Visual Studio Code Based on JsDuck Ducumentation implenting for Sencha Ext JS.",
  "author": "Miguel Yax <mig_dj@hotmail.com>",
  "icon": "images/icon.png",
  "license": "MIT",
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "javascript",
        "aliases": [
          "JavaScript"
        ],
        "extensions": [
          ".js"
        ]
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/short-js-doc.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Krazeus/short-js-doc.git"
  },
  "devDependencies": {
    "express": "^4.15.2"
  }
}